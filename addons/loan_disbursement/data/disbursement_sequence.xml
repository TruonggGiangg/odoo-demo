<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Loan Disbursement -->
        <record id="seq_loan_disbursement" model="ir.sequence">
            <field name="name">Loan Disbursement</field>
            <field name="code">loan.disbursement</field>
            <field name="prefix">DIS/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequence for Loan Application -->
        <record id="seq_loan_application" model="ir.sequence">
            <field name="name">Loan Application</field>
            <field name="code">loan.application</field>
            <field name="prefix">LOAN/%(y)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
